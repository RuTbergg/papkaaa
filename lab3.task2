points = {
    1 : ["A", "E", "I", "L", "N", "O", "R", "S", "T", "U"],
    2: ["D", "G"],
    3: ["B", "C", "M", "P"],
    4: ["F", "H", "V", "W", "Y"],
    5: ["K"],
    8: ["J", "X"],
    10: ["Q", "Z"]
}
sum = 0
word = input("Введите слово ")
for i in word:
    for j in points:
        if i in points[j]:
            sum += int(j)
print(sum)
